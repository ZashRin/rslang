{
  "root": true,
  "plugins": ["prettier", "import"],
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "prettier",
    "eslint:recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 1,
    "class-methods-use-this": "off"
  }
}
